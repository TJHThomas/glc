.popup_tip{
  width: 100%;
  height: 300rpx;
  background:white;
  line-height: 300rpx;
  text-align: center;
}
.wrapper{
  display: flex;
  flex-direction: column;
  background-color: white;
  margin: 16rpx;
  border-radius: 5%;
  padding: 5rpx 20rpx;
}
.wrapper-list{
  border-bottom: 1.5px solid rgba(204, 204, 204, .2);
  margin-bottom: 12rpx;
}
.wrapper-list-image{
  width: 40rpx;
  height: 40rpx;
  margin-top: 6rpx;
}
.wrapper_detail{
  position: absolute;
  z-index: 9999;
  background-color: white;
  border: 1px solid rgba(0, 0, 0, 0.1);
  padding: 20rpx;
  border-radius: 15rpx;
  font-size: 28rpx;
}
.wrapper_b{
  position: fixed;
  bottom: 1%;
  left: 5%;
  margin: auto 0;
  width: 90%;
}
.wrapper_b_btn{
  background-color: black;
  color: white;
  text-align: center;
  padding: 20rpx;
  border-radius: 15rpx;
}